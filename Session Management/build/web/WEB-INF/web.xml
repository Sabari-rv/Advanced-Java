<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginfirst</servlet-name>
        <servlet-class>loginfirst</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cookiesserv1</servlet-name>
        <servlet-class>cookiesserv1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cookiesserv2</servlet-name>
        <servlet-class>cookiesserv2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rewriteserv1</servlet-name>
        <servlet-class>rewriteserv1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rewriteserv2</servlet-name>
        <servlet-class>rewriteserv2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hiddenurl1</servlet-name>
        <servlet-class>hiddenurl1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hiddenurl2</servlet-name>
        <servlet-class>hiddenurl2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginfirst</servlet-name>
        <url-pattern>/loginfirst</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cookiesserv1</servlet-name>
        <url-pattern>/cookiesserv1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cookiesserv2</servlet-name>
        <url-pattern>/cookiesserv2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rewriteserv1</servlet-name>
        <url-pattern>/rewriteserv1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rewriteserv2</servlet-name>
        <url-pattern>/rewriteserv2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hiddenurl1</servlet-name>
        <url-pattern>/hiddenurl1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hiddenurl2</servlet-name>
        <url-pattern>/hiddenurl2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
